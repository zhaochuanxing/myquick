{
  "package": "com.application.demo",
  "name": "myquick",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "101",
  "icon": "/Common/logo.png",
  "features": [
    { "name": "system.prompt" },
    { "name": "system.router" },
    { "name": "system.shortcut" }
  ],
  "permissions": [
    { "origin": "*" }
  ],
  "config": {
    "logLevel": "off"
  },
  "router": {
    "entry": "Demo",
    "pages": {
      "Demo": {
        "component": "index"
      },
      "DemoDetail": {
        "component": "index"
      },
      "Home":{
        "component":"index"
      },
      "Directive":{
        "component":"index"
      },
      "StyleLayout":{
          "component":"index"
      },
      "StyleLayout/style":{
         "component":"index"
      },
      "StyleLayout/less":{
          "component":"index"
      },
      "StyleLayout/layout":{
          "component":"index"
      },
      "StyleLayout/updatestyle":{
          "component":"index"
      },
      "Lifecycle":{
          "component":"index"
      },
      "About": {
        "component": "index"
      },
      "ComponentTabs":{
         "component":"index"
      },
      "CompontentTabs/base/div":{
        "component":"index"
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "Demo": {
        "titleBarText": "示例页",
        "menu": false
      },
      "DemoDetail": {
        "titleBarText": "详情页"
      },

      "Lifecycle":{
        "menu":true
      },
      "About": {
        "menu": false
      }
    }
  }
